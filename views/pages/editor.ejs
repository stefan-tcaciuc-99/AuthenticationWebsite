<div class="form-group row" xmlns="http://www.w3.org/1999/html">
    <label class="col-form-label col-sm-2" for="Title">Email</label>
    <div class="col-sm-8">
        <label>
            <input autofocus class="form-control" name="Email" value="<%= accounts.Email %>" readonly/>
        </label>
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Author">Password</label>
    <div class="col-sm-7">
        <label>
            <input class="form-control" name="Password" value="<%= accounts.Password %>"required/>
        </label>
    </div>
</div>

<div class="form-group row">
    <label class="col-form-label col-sm-2" for="Comments">Role</label>
    <div class="col-sm-10">
        <label>
            <input class="form-control"  name="Role"value="<%= accounts.Role %>"required/>
        </label>
    </div>
</div>